package ch06.sec06.exam07;

public class Data1 {
	
	int value;	
//	Data1(){}   안써도 자동 생성 됨
} 

class Data2{
	int value;
	Data2(int x){     // 정수 벨류 가져와
		value = x;
	}
}	
class ConstructorTest {	

	public static void main(String[] args) {
		Data1 d1 = new Data1();
		Data2 d2 = new Data2(10);
		
	}

}
